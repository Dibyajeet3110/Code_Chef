#include<iostream>
#include<math.h>
using namespace std;
int main()
{int t;
cin>>t;
while(t--)
{
    int n,x,k=0,j=0;
    cin>>n>>x;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    for(int i=0;i<n;i++)
    {
      if(a[i]<80 && a[i]>9)
    {++k;
      }
      else{
          j++;
      }
    }
    cout<<ceil((float)j/x)+ceil((float)k/x)<<endl;
}
}
